@model CUOIKYLTWEBNANGCAO.Models.Party
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Xoá tài khoản";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Xoá</h1>

<h3>Bạn thật sự muốn xoá?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Loại tài khoản
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TypeOfParty)
        </dd>
       
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Username)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Username)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Password)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Password)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="PartyId" />
        <input type="submit" value="Xoá" class="btn btn-danger" /> |
        <a asp-action="Index">Trở về</a>
    </form>
</div>
