<!DOCTYPE html>
<html lang="en">
<head>
    <!--them css-->
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="keywords" content="au theme template">
    <!-- Title Page-->
    <title>Quản lý sản phẩm</title>
    <link rel="icon" href="../../LayoutAdmin/images/icon/logo-mini.png" type="image/icon type">

    <!-- Fontfaces CSS-->
    <link href="../../LayoutAdmin/css/font-face.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="../../LayoutAdmin/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="../../LayoutAdmin/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="../../LayoutAdmin/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="../../LayoutAdmin/css/theme.css" rel="stylesheet" media="all">
    <link rel="stylesheet" href="../../LayoutAdmin/css/style.css">


    <style type="text/css">
        /* Chart.js */
    </style>
</head>
<body class="animsition" style="animation-duration: 900ms; opacity: 1;">

    <div class="page-wrapper">


        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <!-- logo -->
                    <img src="../../LayoutAdmin/images/icon/logo.png" alt="Cool Admin">
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1 ps">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="active has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-tachometer-alt"></i>Sản phẩm
                            </a>
                        </li>
                        <li class="has-sub">
                            <a class="" href=".?act=xuatXml" target="_blank">
                                <i class="fa fa-print"></i>Xuất XML sản phẩm
                            </a>
                        </li>
                        <li class="has-sub">
                            <a class="" href=".?act=xuatXmlLoai" target="_blank">
                                <i class="fa fa-print"></i>Xuất XML Loại
                            </a>
                        </li>

                    </ul>
                </nav>
                <div class="ps__rail-x" style="left: 0px; bottom: 0px;"><div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__rail-y" style="top: 0px; right: 0px;"><div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 0px;"></div></div>
            </div>
        </aside>        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" method="POST">
                                <input onchange="" id="timkiem" class="au-input au-input--xl" type="text" placeholder="Tìm kiếm hàng hóa...">
                                <button onclick="" class="au-btn--submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">
                                <div class="noti-wrap">
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-comment-more"></i>
                                        <span class="quantity">1</span>
                                        <div class="mess-dropdown js-dropdown">
                                            <div class="mess__title">
                                                <p>You have 2 news message</p>
                                            </div>
                                            <div class="mess__item">
                                                <div class="image img-cir img-40">
                                                    <img src="../../LayoutAdmin/images/icon/avatar-06.jpg" alt="Michelle Moreno">
                                                </div>
                                                <div class="content">
                                                    <h6>Michelle Moreno</h6>
                                                    <p>Have sent a photo</p>
                                                    <span class="time">3 min ago</span>
                                                </div>
                                            </div>
                                            <div class="mess__item">
                                                <div class="image img-cir img-40">
                                                    <img src="../../LayoutAdmin/images/icon/avatar-04.jpg" alt="Diane Myers">
                                                </div>
                                                <div class="content">
                                                    <h6>Diane Myers</h6>
                                                    <p>You are now connected on message</p>
                                                    <span class="time">Yesterday</span>
                                                </div>
                                            </div>
                                            <div class="mess__footer">
                                                <a href="#">View all messages</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-email"></i>
                                        <span class="quantity">1</span>
                                        <div class="email-dropdown js-dropdown">
                                            <div class="email__title">
                                                <p>You have 3 New Emails</p>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="../../LayoutAdmin/images/icon/avatar-06.jpg" alt="Cynthia Harvey">
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, 3 min ago</span>
                                                </div>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="../../LayoutAdmin/images/icon/avatar-05.jpg" alt="Cynthia Harvey">
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, Yesterday</span>
                                                </div>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="../../LayoutAdmin/images/icon/avatar-04.jpg" alt="Cynthia Harvey">
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, April 12,,2018</span>
                                                </div>
                                            </div>
                                            <div class="email__footer">
                                                <a href="#">See all emails</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-notifications"></i>
                                        <span class="quantity">3</span>
                                        <div class="notifi-dropdown js-dropdown">
                                            <div class="notifi__title">
                                                <p>You have 3 Notifications</p>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c1 img-cir img-40">
                                                    <i class="zmdi zmdi-email-open"></i>
                                                </div>
                                                <div class="content">
                                                    <p>You got a email notification</p>
                                                    <span class="date">April 12, 2018 06:50</span>
                                                </div>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c2 img-cir img-40">
                                                    <i class="zmdi zmdi-account-box"></i>
                                                </div>
                                                <div class="content">
                                                    <p>Your account has been blocked</p>
                                                    <span class="date">April 12, 2018 06:50</span>
                                                </div>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c3 img-cir img-40">
                                                    <i class="zmdi zmdi-file-text"></i>
                                                </div>
                                                <div class="content">
                                                    <p>You got a new file</p>
                                                    <span class="date">April 12, 2018 06:50</span>
                                                </div>
                                            </div>
                                            <div class="notifi__footer">
                                                <a href="#">All notifications</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <img src="../../LayoutAdmin/images/icon/avatar-01.jpg" alt="Admin">
                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#">Admin</a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="../../LayoutAdmin/images/icon/avatar-01.jpg" alt="John Doe">
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name">
                                                        <a href="#">john doe</a>
                                                    </h5>
                                                    <span class="email">admin@ute.udn.vn</span>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__body">
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-account"></i>Account
                                                    </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-settings"></i>Setting
                                                    </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-money-box"></i>Billing
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href=".?act=logout">
                                                    <i class="zmdi zmdi-power"></i>Logout
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
            <div class="container relative">
                <div id="add_loai" class="absolute none">
                    <div class="alertThem">
                        <p>Thêm Loại</p>
                        <input id="data-add-loai" type="text">
                        <button onclick="">Hủy</button>
                        <button onclick="">Thêm</button>
                    </div>
                </div>
                <div id="xoa_sp" class="absolute none">
                    <div class="alertThem">
                        <p>Xác nhận xóa</p>
                        <button onclick="">Hủy</button>
                        <button onclick="">Xóa</button>
                    </div>
                </div>


                <div id="alert" class="alert none">

                    <span class="closebtn" onclick="">×</span>
                    <strong>Danger!</strong> <i>Indicates a dangerous or potentially negative action.</i>
                </div>

                @RenderBody()
            </div>
        </div>




    </div>



    <!-- Jquery JS-->
    <script src="../../LayoutAdmin/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="../../LayoutAdmin/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="../../LayoutAdmin/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="../../LayoutAdmin/vendor/slick/slick.min.js">
    </script>
    <script src="../../LayoutAdmin/vendor/wow/wow.min.js"></script>
    <script src="../../LayoutAdmin/vendor/animsition/animsition.min.js"></script>
    <script src="../../LayoutAdmin/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="../../LayoutAdmin/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="../../LayoutAdmin/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="../../LayoutAdmin/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="../../LayoutAdmin/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="../../LayoutAdmin/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="../../LayoutAdmin/vendor/select2/select2.min.js">
    </script>
    <!-- Main JS-->
    <script src="../../LayoutAdmin/js/main.js"></script>
    <script src="../../LayoutAdmin/js/script.js"></script>
</body>
</html>